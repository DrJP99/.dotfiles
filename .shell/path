# PATH

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/bin" ] ; then
    PATH="$PATH:$HOME/bin"
fi

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/.local/bin" ] ; then
    PATH="$PATH:$HOME/.local/bin"
fi

# Set PATH to include spicetify if it exists
if [ -d "$HOME/.spicetify" ]; then
    PATH="$PATH:$HOME/.spicetify"
fi